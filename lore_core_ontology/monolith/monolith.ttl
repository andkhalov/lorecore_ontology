###  https://lorecore.ai/ont/core.ttl#Evaluation
:Evaluation rdf:type owl:Class ;
            rdfs:subClassOf :Inference ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :accuracyScore ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange [ rdf:type rdfs:Datatype ;
                                                owl:onDatatype xsd:decimal ;
                                                owl:withRestrictions ( [ xsd:minInclusive 0.0
                                                                       ]
                                                                       [ xsd:maxInclusive 1.0
                                                                       ]
                                                                     )
                                              ]
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :completenessScore ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange [ rdf:type rdfs:Datatype ;
                                                owl:onDatatype xsd:decimal ;
                                                owl:withRestrictions ( [ xsd:minInclusive 0.0
                                                                       ]
                                                                       [ xsd:maxInclusive 1.0
                                                                       ]
                                                                     )
                                              ]
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :confidenceScore ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange [ rdf:type rdfs:Datatype ;
                                                owl:onDatatype xsd:decimal ;
                                                owl:withRestrictions ( [ xsd:minInclusive 0.0
                                                                       ]
                                                                       [ xsd:maxInclusive 1.0
                                                                       ]
                                                                     )
                                              ]
                            ] ,
                            [ rdf:type owl:Restriction ;
